<h1>Create New Question</h1>

<form action="tests/<%=params[:test_id]%>/questions" method="post">
  <input name="authenticity_token" value="<%= form_authenticity_token %>" type="hidden">
  <p>
    <label for="question-title">Body:</label>
    <input type="text" name="question[body]" id="question-title">
  </p>

  <label for="question-test_id">test_id:</label>
  <input type="integer" name="question[test_id]" id="question-test_id">

  <br>

  <input type="submit" value="Create">
</form>
