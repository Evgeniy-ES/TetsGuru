
<div class="container px-2">
  <h1><%= t('tests.index.header') %></h1>

  <p>
  <table class="table">
    <tr>
      <th class="sort-by-title pointer">
      <%= t('tests.index.title') %>
      <%= octicon 'arrow-up', class: 'text-success hide' %>
      <%= octicon 'arrow-down', class: 'text-success hide' %>
      </th>
      <th><%= t('tests.index.level') %></th>
      <th><%= t('tests.index.count questions') %></th>
      <th><%= t('tests.index.action') %></th>
    </tr>

    <%= render @tests %>
  </table>
</div>
